#include<iostream>
using namespace std;

int main(){
    int a,b,w,x,y,z;
    a = 5;
    cout << "Enter b: ";
    cin >> b;
    w = a+b;
    x = a-b;
    y = a*b;
    z = a/b;
    cout << "a plus b equals " << w << "\n";
    cout << "a minus b equals " << x << "\n";
    cout << "a times b equals " << y << "\n";
    cout << "a divided by b equals " << z;
}
